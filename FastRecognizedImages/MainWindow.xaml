<Window x:Class="FastRecognizedImages.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FastRecognizedImages"
        xmlns:ui="clr-namespace:FastRecognizedImages.UI"
        mc:Ignorable="d"
        Style="{StaticResource MyWindowStyle}"
        Title="FastRecognizedImages" Height="350" Width="525">
    <Grid>
        <TabControl Padding="0">
            <TabItem Header="DRAW">
                <ui:DrawingLayout x:Name="Draw"/>
            </TabItem>
            <TabItem Header="CONSOLE">
                <ui:ConsoleLayout x:Name="console"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
